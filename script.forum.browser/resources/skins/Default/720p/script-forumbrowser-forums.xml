<window>
  <defaultcontrol>120</defaultcontrol>
  <coordinates>
    <system>1</system>
    <posx>0</posx>
    <posy>0</posy>
  </coordinates>
  <animation effect="fade" time="250">WindowOpen</animation>
  <animation effect="fade" time="250">WindowClose</animation>
  <controls>
    <control type="group">
      <posx>100</posx>
      <posy>10</posy>
      <control type="image">
        <posx>0</posx>
        <posy>0</posy>
        <width>1090</width>
        <height>700</height>
        <texture border="40">default-panel.png</texture>
      </control>
      <!-- ** Required ** Do not change <id> or <type> -->

        <control type="image" >
            <description>page header tab icon</description>
            <posx>20</posx>
            <posy>11</posy>
            <width>518</width>
            <height>100</height>
            <texture>logo.png</texture>
        </control>
             


      <control type="label">
        <description>textarea</description>
        <posx>580</posx>
        <posy>30</posy>
        <width>230</width>
        <height>60</height>
        <font>font18</font>
        <textcolor>white</textcolor>
        <aligny>center</aligny>
        <align>center</align>
        <label>Forums</label>
        <wrapmultiline>true</wrapmultiline>
      </control>
      
      <control type="button" id="200">
       <description>The Button</description>
       <onleft>120</onleft>
       <onright>201</onright>
       <onup>120</onup>
       <ondown>120</ondown>
       <posx>30</posx>
       <posy>620</posy>
       <width>330</width>
       <height>40</height>
       <visible>true</visible>
       <colordiffuse>FFFFFFFF</colordiffuse>
       <font>font18</font>
       <align>center</align>
       <label>Settings</label>
       <textcolor>FFFFFFFF</textcolor>
       <disabledcolor>80FFFFFF</disabledcolor>
      </control>
      
      <control type="button" id="201">
       <description>The Button</description>
       <onleft>200</onleft>
       <onright>202</onright>
       <onup>120</onup>
       <ondown>120</ondown>
       <posx>384</posx>
       <posy>620</posy>
       <width>330</width>
       <height>40</height>
       <visible>true</visible>
       <colordiffuse>FFFFFFFF</colordiffuse>
       <font>font18</font>
       <align>center</align>
       <label>Subscriptions</label>
       <enable>false</enable>
       <textcolor>FFFFFFFF</textcolor>
       <disabledcolor>80FFFFFF</disabledcolor>
      </control>
      
      <control type="button" id="202">
       <description>The Button</description>
       <onleft>201</onleft>
       <onright>120</onright>
       <onup>120</onup>
       <ondown>120</ondown>
       <posx>738</posx>
       <posy>620</posy>
       <width>330</width>
       <height>40</height>
       <visible>true</visible>
       <colordiffuse>FFFFFFFF</colordiffuse>
       <font>font18</font>
       <align>center</align>
       <label>Private Messages</label>
       <enable>false</enable>
       <textcolor>FFFFFFFF</textcolor>
       <disabledcolor>80FFFFFF</disabledcolor>
      </control>
      
      <control type="image" >
            <description>page header tab icon</description>
            <posx>844</posx>
            <posy>11</posy>
            <width>213</width>
            <height>100</height>
            <aspectratio>keep</aspectratio>
            <texture>$INFO[Container(120).ListItem.Property(image)]</texture>
      </control>
      
      <control type="label">
        <description>textarea</description>
        <posx>25</posx>
        <posy>535</posy>
        <width>1055</width>
        <height>45</height>
        <font>font18</font>
        <textcolor>white</textcolor>
        <aligny>center</aligny>
        <align>center</align>
        <wrapmultiline>true</wrapmultiline>
        <label>$INFO[Container(120).ListItem.Property(description)]</label>
      </control>
      
      <control type="image">
        <posx>20</posx>
        <posy>120</posy>
        <width>1060</width>
        <height>2</height>
        <texture>TRseparator.png</texture>
      </control>
      <control type="image">
        <posx>20</posx>
        <posy>520</posy>
        <width>1060</width>
        <height>2</height>
        <texture>TRseparator.png</texture>
      </control>
      <control type="image">
        <posx>20</posx>
        <posy>524</posy>
        <width>1060</width>
        <height>2</height>
        <texture>TRseparator2.png</texture>
      </control>      
      <control type="image">
        <posx>20</posx>
        <posy>596</posy>
        <width>1060</width>
        <height>2</height>
        <texture>TRseparator2.png</texture>
      </control> 
      <control type="image">
        <posx>20</posx>
        <posy>600</posy>
        <width>1060</width>
        <height>2</height>
        <texture>TRseparator.png</texture>
      </control>      

      <!-- ** Required ** Do not change <id> or <type> -->
      <control type="list" id="120">
        <posx>20</posx>
        <posy>132</posy>
        <width>1040</width>
        <height>380</height>
        <onleft>202</onleft>
        <onright>200</onright>
        <onup>120</onup>
        <ondown>120</ondown>
        <viewtype label="535">list</viewtype>
        <pagecontrol>121</pagecontrol>
        <scrolltime>200</scrolltime>
        <itemlayout height="38">
          <control type="label">
            <posx>10</posx>
            <posy>0</posy>
            <width>990</width>
            <height>38</height>
            <font>font18</font>
            <aligny>center</aligny>
            <textcolor>white</textcolor>
            <selectedcolor>green</selectedcolor>
            <align>left</align>
            <info>ListItem.Label</info>
            <visible>StringCompare(ListItem.Genre,bold)</visible>
          </control>
          
          <control type="label">
            <posx>10</posx>
            <posy>0</posy>
            <width>990</width>
            <height>38</height>
            <font>font18</font>
            <aligny>center</aligny>
            <textcolor>FFDDBBBB</textcolor>
            <selectedcolor>green</selectedcolor>
            <align>left</align>
            <info>ListItem.Label</info>
          </control>          
        </itemlayout>
        <focusedlayout height="38">
          <control type="image">
            <posx>0</posx>
            <posy>0</posy>
            <width>1040</width>
            <height>38</height>
            <texture border="10">default-list-focus.png</texture>
            <visible>Control.HasFocus(120)</visible>
          </control>
          <control type="label">
            <posx>10</posx>
            <posy>0</posy>
            <width>990</width>
            <height>38</height>
            <font>font18</font>
            <aligny>center</aligny>
            <textcolor>white</textcolor>
            <selectedcolor>green</selectedcolor>
            <align>left</align>
            <info>ListItem.Label</info>
            <visible>StringCompare(ListItem.Genre,bold)</visible>
          </control>
          
          <control type="label">
            <posx>10</posx>
            <posy>0</posy>
            <width>990</width>
            <height>38</height>
            <font>font18</font>
            <aligny>center</aligny>
            <textcolor>FFDDBBBB</textcolor>
            <selectedcolor>green</selectedcolor>
            <align>left</align>
            <info>ListItem.Label</info>
          </control>
        </focusedlayout>
      </control>
      <control type="group">
        <posx>1055</posx>
        <posy>132</posy>
        <control type="scrollbar" id="121">
          <description>scroll bar indicator for lists</description>
          <posx>0</posx>
          <posy>0</posy>
          <width>12</width>
          <height>380</height>
          <texturesliderbackground border="6">default-scroll_back.png</texturesliderbackground>
          <texturesliderbar border="6">default-scroll_mid.png</texturesliderbar>
          <texturesliderbarfocus border="6">default-scroll_mid.png</texturesliderbarfocus>
          <textureslidernib>-</textureslidernib>
          <textureslidernibfocus>-</textureslidernibfocus>
          <orientation>vertical</orientation>
          <showonepage>false</showonepage>
          <visible>Control.IsVisible(120)</visible>
        </control>
      </control>

    </control>
  </controls>
</window>
